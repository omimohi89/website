  background-position: center;
  background-size: cover;
  z-index: 1; /* Update the z-index to be above the ::before pseudo-element */
}

.landingpage::before {
  content: '';
  position: absolute;
  top: -10;
  left: 100%;
  width: 700px; /* Adjust the width as needed */
  height: 700px; /* Adjust the height as needed */
  background-color: #FF78D9; /* Adjust the background color as needed */
  transform: translate(-50%, -50%);
  border-radius: 50%;
  filter: blur(140px);
  z-index: 2; /* Update the z-index to be above the landingpage background */
}

.landingpage p,
.landingpage .h1 {
  position: relative;
  z-index: 3; /* Ensure the text appears above the ::before pseudo-element */
  /* Other styles for the text elements */
}








